	xmlns:mdl="http://www.cds.caltech.edu/erato/schemas/root">

<annotation>
  <documentation>
     File name   : root.xsd
     Description : The Root Schema
     Author(s)   : Michael Hucka <mhucka@caltech.edu>
     Organization: Caltech ERATO group
     Language    : XML Schema

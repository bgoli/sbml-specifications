              <ci> period </ci>
            </bvar>
            <bvar>
              <ci> center </ci>
            </bvar>
            <bvar>
              <ci> amplitude </ci>
            </bvar>
            <apply>
              <plus/>
              <apply>
                <times/>
                <ci> amplitude </ci>
                <apply>
                  <gt/>
                  <apply>
                    <sin/>
                    <apply>
                      <divide/>
                      <apply>
                        <times/>
                        <cn type="integer"> 2 </cn>
                        <cn> 3.1414926 </cn>
                        <ci> time </ci>
                      </apply>
                      <ci> period </ci>
                    </apply>
                  </apply>
                  <cn type="integer"> 0 </cn>
                </apply>
              </apply>
              <ci> center </ci>
            </apply>
          </lambda>
